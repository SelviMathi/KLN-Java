1. Print the following pattern
*****
*****
*****
*****
*****
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int limit,row,col;
	    Scanner obj = new Scanner(System.in);
	    limit=obj.nextInt();
	    for(row=1;row<=limit;row++)
	    {
	        for(col=1;col<=limit;col++)
	      	    System.out.print("*");
	      	    System.out.println();
	    }
}
}

2. Print the following pattern
11111
22222
33333
44444
55555
******************************************************************************
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int limit,row,col;
	    Scanner obj = new Scanner(System.in);
	    limit=obj.nextInt();
	    for(row=1;row<=limit;row++)
	    {
	        for(col=1;col<=limit;col++)
	      	    System.out.printf("%d",row);
	      	    System.out.println();
	    }
}
}
3. Print the following pattern
12345
12345
12345
12345
12345
******************************************************************************
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int limit,row,col;
	    Scanner obj = new Scanner(System.in);
	    limit=obj.nextInt();
	    for(row=1;row<=limit;row++)
	    {
	        for(col=1;col<=limit;col++)
	      	    System.out.printf("%d",col);
	      	    System.out.println();
	    }
}
}

4. Print the following pattern
ABCDE
ABCDE
ABCDE
ABCDE
ABCDE
/******************************************************************************
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int limit,row,col;
	    Scanner obj = new Scanner(System.in);
	    limit=obj.nextInt();
	    for(row=1;row<=limit;row++)
	    {
	        int val=65;
	       for(col=1;col<=limit;col++)
	          System.out.printf("%c",val++);
	    System.out.println();
	    }
}
}
